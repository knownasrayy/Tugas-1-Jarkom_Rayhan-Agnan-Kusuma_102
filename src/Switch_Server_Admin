enable
configure terminal

! --- 1. Buat VLAN 50 ---
vlan 50
 name Server_Admin
 exit

! --- 2. Port yang ke Router_Pusat ---
! (Asumsi kamu pakai port Fa0/1 di switch ini)
interface FastEthernet0/1
 description Ke-Router_Pusat-Fa0/3/1
 switchport mode access
 switchport access vlan 50
 no shutdown
 exit

! --- 3. Port yang ke Server/PC Admin ---
! (Asumsi PC terhubung ke port Fa0/2)
interface FastEthernet0/2
 description Ke-PC-Admin
 switchport mode access
 switchport access vlan 50
 no shutdown
 exit

end
write memory
